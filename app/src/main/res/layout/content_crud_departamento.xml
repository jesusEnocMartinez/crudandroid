<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".CrudDepartamento"
    tools:showIn="@layout/activity_crud_departamento">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Crud Departamento"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="35sp"
        tools:layout_editor_absoluteX="52dp"
        tools:layout_editor_absoluteY="36dp">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ingresa tu correo"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="332dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Telefono"
        tools:layout_editor_absoluteX="90dp"
        tools:layout_editor_absoluteY="281dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre departamento"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="214dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="104dp"
        android:layout_height="51dp"
        android:backgroundTint="#11E4DC"
        android:text="Insertar"
        tools:layout_editor_absoluteX="75dp"
        tools:layout_editor_absoluteY="497dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#11E4DC"
        android:text="Actualizar"
        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="497dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="103dp"
        android:layout_height="49dp"
        android:backgroundTint="#11E4DC"
        android:text="Borrar"
        tools:layout_editor_absoluteX="77dp"
        tools:layout_editor_absoluteY="578dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="110dp"
        android:layout_height="49dp"
        android:backgroundTint="#11E4DC"
        android:text="Buscar"
        tools:layout_editor_absoluteX="226dp"
        tools:layout_editor_absoluteY="578dp" />

    <EditText
        android:id="@+id/id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="164dp"
        tools:layout_editor_absoluteY="142dp" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="164dp"
        tools:layout_editor_absoluteY="201dp" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="165dp"
        tools:layout_editor_absoluteY="264dp" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="165dp"
        tools:layout_editor_absoluteY="320dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="119dp"
        android:layout_height="27dp"
        android:text="IDE departamento"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="151dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="419dp"
        android:layout_height="59dp"
        android:backgroundTint="#2196F3"
        tools:layout_editor_absoluteX="-4dp"
        tools:layout_editor_absoluteY="681dp" />


</androidx.constraintlayout.widget.ConstraintLayout>